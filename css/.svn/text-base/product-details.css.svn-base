	padding: 4px 9px;
}

.pane {
	position:absolute;
	top:0px;
	left:0px;
	opacity:0;
	visibility:hidden;
	-webkit-transition:all .5s ease-in-out;
}

.pane.first img{
	margin-left: 0;
	margin-top: 0;
}

.pane img{
	margin-top: 50px;
	margin-left: 50px;
	opacity:0;
	visibility:hidden;
}

.pane .video{
	margin-top:40px;
	margin-left:30px;
}

.pane .video img{
	margin-top:0px;
	margin-left:0px;
}

.pane.active {
	opacity:1;
	visibility:visible;
	-webkit-transition:all .5s ease-in-out;
}

.pane.active img{
	opacity: 1;
	visibility: visible;
	-webkit-transition:all .5s ease-in-out;
}

