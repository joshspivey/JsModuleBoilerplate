/* tooltips */
.tooltip-trigger.active {
	color: #d14179;
}

.tooltip-content {
	display: none;
	position: absolute;
	color: #cc2466;
}

.social div.tooltip-content {
	top: 32px;
	right: 20px;
	border: 1px solid #d4d4d4;
	background-color: #fbfbfb;
}

.social div.tooltip-content > div {
	width: 100%;
