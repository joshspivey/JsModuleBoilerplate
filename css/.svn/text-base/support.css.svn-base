/* Regular Form styles */
.form label {
	width:400px;
	clear:both;
	display:block;
	margin:20px 0px;
	zoom: 1;
}

.form label:before, .form label:after {
	content: "\0020";
	display: block;
	height: 0;
}

.form label:after {
	clear: both;
}

.form input {
	width:157px;
	height:20px;
	background:#FFFFFF;
	border:1px solid #cccccc;
	font-family: Arial, Helvetica, sans-serif;
	font-size:10px;
