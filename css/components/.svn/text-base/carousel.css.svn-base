}

/* Carousel 6 */

.carousel6 {
	width: 100%;
	margin-top: 10px;
	height: 60px;
	padding: 0;
}

.carousel6 ul {
	margin-left: 0;
}

.carousel6 li {
	width: 230px;
}

.carousel6 li a.img-link:link, .carousel6 li a.img-link:visited, .carousel6 .content {
	width: 100px;
	margin-right: 0;
}

.carousel6 .carousel-arrow-left a, .carousel6 .carousel-arrow-right a {
	margin-top:20px;
}

.carousel6 li .content {
	clear: none;
}

.carousel6 .carousel-mask {
	float: left;
	width: 95%;
	height: 100%;
	left: 26px;
	overflow: hidden;
	position: absolute;
}

/* Carousel 7 */
