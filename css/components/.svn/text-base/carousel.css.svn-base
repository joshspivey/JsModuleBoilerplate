 */

/* Carousel 2 */
.carousel2 {
	height: 315px;
	overflow: hidden;
}

.carousel2 h2 {
	text-transform: uppercase;
	font-size: 15px;
}

.carousel2 ul {
	margin-left: 60px;
}

.carousel2 ul li {
	width: 940px;
	height: 340px;
	margin-left: 0px;
}

.carousel2 .carousel-arrow-right a, .carousel2 .carousel-arrow-left a {
	margin-top: 150px;
}

.carousel2 .content {
	width: 420px;
	font-size: 12px;
	float: left;
}

.carousel2 .twit-name strong {
	font-size: 14px;
	font-weight: bold;
}

.carousel2 .twit-feed {
	padding-right: 60px;
	background: url("/common/images/global/lg-global-sprite.png") no-repeat 370px -470px;
}

.carousel2 .button-row {
	float: left;
	width: 100%;
	padding-top: 25px;
}

	.lt-ie9 .carousel2 .button-row a.button.small{
		margin-left: -80px;
	}

.carousel2 .button-row .twitter-button {
	float: left;
}

.carousel2 .button-row .facebook-recommend {
	float: left;
	padding-bottom: 6px;
	margin-left: 13px;
}

.carousel2 h3 {
	font-size: 24px !important;
	padding-top: 5px;
	padding-bottom: 17px;
	color: #D80546;
}

.carousel2 p {
	margin-bottom: 0;
	line-height: 18px;
}

.carousel2 image {
	float: left;
}

.carousel2 .content a.button {
	font-size: 11px;
	color: #fff;
	float: right;
	margin-right: 25px;
}

/* Carousel 3 */

.carousel3 {
	height: 77px;
	width: 950px;
	padding-top: 0px;
	border-bottom: 1px solid #CCCCCC;
}

.carousel3 ul li {
	cursor: pointer;
	width: 180px;
	text-align: center;
	padding-bottom: 20px;
	background: url("/common/images/global/lg-global-sprite.png") no-repeat -102px -480px;
	-webkit-transition: all .3s ease-in-out;
}

.carousel3 ul li.active {
	background: url("/common/images/global/lg-global-sprite.png") no-repeat -102px -494px;
	-webkit-transition: all .3s ease-in-out;
}

.carousel3 .carousel-arrow-right a, .carousel3 .carousel-arrow-left a {
	margin-top: 30px;
}

.carousel3 .carousel-arrow-right, .carousel3 .carousel-arrow-left {
	background: none;
}

.carousel3 .carousel-arrow-left {
	margin-left: 12px;
}

.carousel3 .carousel-arrow-left a, .carousel3 .carousel-arrow-right a {
	width: 9px;
	height: 14px;
	background: url("/common/images/global/lg-global-sprite.png") no-repeat 0 -722px;
}

.carousel3 .carousel-arrow-right a {
	background-position: -9px -722px;
}

.carousel3 .carousel-arrow-left.disabled a{
	background-position: 0px -736px;
}

.carousel3 .carousel-arrow-right.disabled a{
	background-position: -9px -736px;
}

/* Carousel 4 */

.carousel4 {
	height: 302px;
	width: 960px;
	padding: 0px;
}

.carousel4 ul {
	margin-left: 0px;
}

.carousel4 ul li{
	width: 961px;
	height: 304px;
	margin: 0px;
	background: url("/us/images/promos/find-bg1.jpg");
}

.carousel4 .carousel-arrow-right, .carousel4 .carousel-arrow-left {
	float: none;
	position: absolute;
	background: none;
}

.carousel4 .carousel-arrow-right {
	top: 160px;
	left: 870px;
}

.carousel4 .carousel-arrow-left {
	top: 160px;
	left: 820px;
}

.carousel4 .carousel-arrow-left a, .carousel4 .carousel-arrow-right a {
	width: 50px;
	height: 20px;
	background: #e01b50;
	text-indent: 0px;
	line-height: 20px;
	color: #fff;
	font-size: 10px;
	text-transform: uppercase;
	text-decoration: none;
	text-align: center;
}

.carousel4 .copyArea {
	position: relative;
	top: 20px;
	left: 50px;
}

.carousel4 .copyArea h2 {
	color: #e01b50;
	font-size: 30px;
	font-weight: normal;
	text-transform: uppercase;
	border: 0px;
}

.carousel4 .copyArea h3 {
	font-size: 20px;
	font-weight: normal;
	text-transform: uppercase;
}

.carousel4 .indicator ul, .indicator label {
	float: left;
	line-height: 14px;
}

.carousel4 .indicator {
	margin-top: 12px;
	margin-left: 12px;
}

.carousel4 .indicator ul li {
	float: left;
	width: 14px;
	height: 14px;
	margin: 0px 2px;
	padding: 0px;
	line-height: 14px;
	text-align: center;
	font-size: 8px;
	background: #c5c5c5;
	list-style: none;
	color: #fff;
}

.carousel4 .indicator ul li.active {
	background: #e33434;
}

.carousel4 .question {
 	position: absolute;
	top: 100px;
	left: 200px;
 }

.carousel4 .question ul li {
	font-family: "BryantLGregular";
	list-style: none;
	margin: 12px;
 }

.carousel4 .question ul li span {
 	display: block;
	float: left;
	width: 12px;
	height: 12px;
	background: #c5c5c5;
	margin-right: 8px;
 }

.carousel4 .question ul li.active span {
	background: #e33434;
}

/* Carousel 5 */

.carousel5 {
