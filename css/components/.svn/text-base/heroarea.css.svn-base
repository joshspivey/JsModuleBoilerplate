/**
 * 	HERO AREA PANELS (.hero)
 */
.hero {
	display:none;
	position:absolute;
	background-repeat: no-repeat;
	float:left;
	top:0px;
	width:960px;
	height:456px;
	-webkit-transition:opacity 3s linear .1s;
	-moz-transition:opacity 3s linear .1s;
	-ms-transition:opacity 3s linear .1s;
}

.hero.heroform{
	-webkit-transition:opacity .5s linear .1s;
	-moz-transition:opacity .5s linear .1s;
	-ms-transition:opacity .5s linear .1s;
}

.hero img {
	border: 0px;
	position: absolute;
	top: 0px;
	left: 0px;
	cursor: pointer;
}

.hero.inactive {
	left: -1000px;
	opacity: 0;
}

.hero.active {
	left: 0px;
	opacity: 1;
}

.hero div.imagearea {
	position:absolute;
	font-family: "BryantLGMedium", Arial, Helvetica, sans-serif; /* full tag needed for dynamic widths in FF */
	text-align:right;
	padding:18px;
	-webkit-border-radius: 7px;
}

.headline-wrapper{
	overflow:hidden;
	text-align:left;
}


.hero .herocopy-wrapper{
	white-space:nowrap;
	overflow:hidden;
	float:left;/*Fix rightside alignment in IEs and incorrect alignment in webkit SV23 09122011*/
	text-align:left;
	margin-top:4px;
	padding-bottom: 5px;
}

.hero .callout-wrapper {
	float: none;
	text-align: left;
	padding-top: 26px;
	text-transform: uppercase;
}

.hero .callout-wrapper p{
	font-family:'BryantLGMedium';
	margin-bottom:5px;
}

.hero .callout-wrapper ul{
	list-style: none;
	margin:0 auto;
}

.hero .callout-wrapper li{
	background:url("/common/images/global/lg-global-sprite.png") -274px -142px no-repeat;
	color:#D80546;
	margin:4px;
	padding-left:12px;
	line-height:14px;
}

.hero .callout-wrapper li a{
	text-align:left;
	text-decoration:none;
	color:#D80546;
	font-size:14px;
}

.hero .callout-wrapper li a:hover{
	text-align:left;
	text-decoration:underline;
	color:#D80546;
}

.hero h2 {
	font-weight:normal;
	font-size: 28px;
	color:#D80546;
	margin:0px;
	padding:0px;
	line-height:30px;
	letter-spacing:-1px;
}

.hero h2 span {
	color:#D80546;
}

.hero p {
	font-size:11px;
	margin-top:-30px;
	-webkit-transition:all 1s ease-in .1s;
	-moz-transition:all 1s ease-in .1s;
	-ms-transition:all 1s ease-in .1s;
}

.hero.brighttext p {
	color:#ccc;
}

.hero .button {
	/* ALL hero buttons align to the left now. */
	float: left;

	-webkit-transition: opacity .5s linear 2s,
	                    -webkit-box-shadow .3s linear,
						text-shadow .3s linear;
	   -moz-transition: opacity .5s linear 2s,
	                    -webkit-box-shadow .3s linear,
					    text-shadow .3s linear;
	    -ms-transition: opacity .5s linear 2s,
		                -webkit-box-shadow .3s linear,
						text-shadow .3s linear;
	opacity: 0;
}

.hero.heroform .button{
	-webkit-transition: opacity .5s linear .6s,
	                    -webkit-box-shadow .3s linear,
						text-shadow .3s linear;
	   -moz-transition: opacity .5s linear .6s,
	                    -webkit-box-shadow .3s linear,
					    text-shadow .3s linear;
	    -ms-transition: opacity .5s linear .6s,
		                -webkit-box-shadow .3s linear,
						text-shadow .3s linear;
}

.hero h2 {
	-webkit-transition:margin-left 1s ease-in-out .1s,opacity 1s linear .1s;
	-moz-transition:margin-left 1s ease-in-out .1s,opacity 1s linear .1s;
	text-align:left;
	white-space:nowrap;
}

.hero.inactive h2 {
	margin-left:286px;
	opacity:0;
}

.hero.active h2{
	margin-left:0px;
	opacity:1;
}
.lt-ie9 .hero.active h2 {
	margin-right: 15px;/*For some reason IE did not want to left align the H2s on the first slide with the other text. This fixes that SV21 09122011*/
}

.hero.active p {
	margin: 0;
	-webkit-transition: margin-top 1s ease-in-out .6s;
	   -moz-transition: margin-top 1s ease-in-out .6s;
	    -ms-transition: margin-top 1s ease-in-out .6s;
}

.hero.heroform.active p {
	-webkit-transition: margin-top .3s ease-in-out .3s;
	   -moz-transition: margin-top .3s ease-in-out .3s;
	    -ms-transition: margin-top .3s ease-in-out .3s;
}

.hero.active .button{
	opacity: 1;
}

#indicator, #hero-nav {
	position:absolute;
	top:12px;
	left:12px;
	cursor:pointer;
}

#indicator ul li {
	float:left;
	margin:2px;
	padding:0px;
	width:12px;
	height:12px;
	line-height:12px;
	text-align:center;
	-webkit-border-radius: 6px;
	border-top-left-radius: 6px;
	border-top-right-radius: 6px;
	border-bottom-left-radius: 6px;
	border-bottom-right-radius: 6px;
	font-size:8px;
	background:#c5c5c5;
	list-style:none;
	color:#fff;
}

#hero-nav {
	position:relative;
	width:100%;
	top: 0;
	left: 0;
	height: 55px;
	padding-left: 15px;
	cursor: pointer;
	background: -moz-linear-gradient(top,#DEDEDE,white 45px);
	background: -webkit-gradient(linear,0 0,0 45,from(#ECECEC),to(white));
	background-repeat: no-repeat;
}

#hero-nav ul li {
	float: left;
	margin: 2px;
	padding: 0;
	height: 20px;
	margin-top: 20px;
	margin-bottom: 15;
	padding-right: 10px;
	margin-right: 10px;
	font-size: 14px;
	list-style: none;
	color: #5D5C5C;
	border-right: 1px dotted #444;
}

	#hero-nav span.indicator-number{
		float:left;
		padding:0px;
		margin-top: -2px;
		margin-right: 5px;
		width:16px;
		height:16px;
		line-height:14px;
		text-align:center;
		-webkit-border-radius: 9px;
		border-top-left-radius: 9px;
		border-top-right-radius: 9px;
		border-bottom-left-radius: 9px;
		border-bottom-right-radius: 9px;
		font-size:8px;
		border: 1px solid #666;
		list-style:none;
		color:#666;
		font-weight: bold;
		font-family: Arial;
		font-size: 10px;
	}
	
	#hero-nav li.active span.indicator-number {
		background:#D30E43;
		border: 1px solid #D30E43;
		color: #fff;
	}

#indicator ul li.active{
	background:#e33434;
}

.hero .callout-wrapper {
	clear: left;
	text-align: left;
	padding-top: 26px;
	text-transform: uppercase;
}

#hero-nav ul li.active, #hero-nav ul li:hover {
	cursor: pointer;
	color: #D80546;
}

#hero-nav .button {
	position: absolute;
	right: 40px;
	top: 15px;
}

