#hero-area{
	position:relative;
	width:960px;
	height:420px;
	overflow:hidden;
	margin-bottom: 5px;
	border-bottom: 1px solid #cccccc;
	font-family: "BryantLGMedium", Arial, Helvetica, sans-serif; /* full tag needed for dynamic widths in FF */
}

#hero-area.tab {
	height: 456px;
	margin-bottom: 20px;
}

/* Removed for now - Hero images will have white space at top
#hero-area.nav .hero {
	margin-top:55px;
}
*/

#hero-area .tabs{
	position:absolute;
	bottom:-160px;
	cursor:pointer;
	font-size:11px;
	-webkit-transition:bottom .7s ease-in-out 1.3s;
	-moz-transition:bottom .7s ease-in-out 1.3s;
	-ms-transition:bottom .7s ease-in-out 1.3s;
}

#hero-area .tabs div{
	position:relative;
	top:0px;
	width:291px;
	height:168px;
	padding:0px;
	float:left;
	padding:14px;
	margin-left:-1px;
	border-top-left-radius:0px;
	border-top-right-radius:0px;
	border-right:1px dotted #cccccc;
	border-left:1px solid #cccccc;

	-webkit-transition: top .5s ease-in,  opacity .5s linear,border-top-left-radius .5s ease-in, border-top-right-radius .5s ease-in, box-shadow .5s linear;
	-moz-transition: top .5s ease-in,  opacity .5s linear,border-top-left-radius .5s ease-in, border-top-right-radius .5s ease-in, box-shadow .5s linear;
	-ms-transition: top .5s ease-in,  opacity .5s linear,border-top-left-radius .5s ease-in, border-top-right-radius .5s ease-in, box-shadow .5s linear;

	background: #fff;
	background: -webkit-gradient(
		linear,
		left top,
		left bottom,
		color-stop(0, rgb(255, 255, 255)),
		color-stop(0.25, rgb(243, 241, 241))
	);
	background: -moz-linear-gradient(
		center top,
		rgb(255, 255, 255) 0%,
		rgb(243, 241, 241) 25%
	);
}

#hero-area .tabs div.active {
	top: -160px;
	z-index: 10;
	border-top-left-radius: 7px;
	border-top-right-radius: 7px;
	-webkit-box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.5);
	-moz-box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.5);
	box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.5);
	border-right:1px solid #cccccc;
	border-left:1px solid #cccccc;

	-webkit-transition: top .5s ease-out,  opacity .5s linear,border-top-left-radius .5s ease-out,border-top-right-radius .5s ease-out, box-shadow .5s linear;
	-moz-transition: top .5s ease-out,  opacity .5s linear,border-top-left-radius .5s ease-out,border-top-right-radius .5s ease-out, box-shadow .5s linear;
	-ms-transition: top .5s ease-out,  opacity .5s linear,border-top-left-radius .5s ease-out,border-top-right-radius .5s ease-out, box-shadow .5s linear;

	/* first, a straight gradient from top to bottom */
	background: -webkit-gradient(linear, left top, left bottom, from(#fff), to(#f3f2f1)); /* Saf4+, Chrome */
	background: -webkit-linear-gradient(top, #fff, #f3f2f1); /* Chrome 10+, Saf5.1+ */
	background:    -moz-linear-gradient(top, #fff, #f3f2f1); /* FF3.6 */
	background:     -ms-linear-gradient(top, #fff, #f3f2f1); /* IE10 */
	background:      -o-linear-gradient(top, #fff, #f3f2f1); /* Opera 11.10+ */
	background:         linear-gradient(top, #fff, #f3f2f1);
	/*filter: progid:DXImageTransform.Microsoft.gradient(startColorStr='#ffffff', EndColorStr='#f3f2f1'); /* IE6â€“IE9 */*/
	/* then, color stops w/ opacity where supported */
	background: -webkit-gradient(
		linear,
		left top,
		left bottom,
		color-stop(0.83, rgba(255, 255, 255, 0.95)),
		color-stop(1, rgba(242, 241, 241, 0.95))
	);
	background: -moz-linear-gradient(
		center top,
		rgba(255, 255, 255, 0.95) 83%,
		rgba(242, 241, 241, 0.95) 100%
	);
}

.ie9 #hero-area .tabs div.active, .lt-ie9 #hero-area .tabs div.active{
	background:#f3f2f1;
}

#hero-area .tabs div .plusminus{
	margin:0px;
	padding:0px;
	position:absolute;
	width:8px;
	height:9px;
	left:12px;
	top:16px;
	cursor:pointer;
	background:url("/common/images/global/lg-global-sprite.png") 0 -128px no-repeat;
}

#hero-area .tabs div.active .plusminus{
	background-position: 0px -137px;
}

#hero-area .tabs div .xout{
	display:none;
	margin:0px;
	padding:0px;
	position:absolute;
	width:16px;
	height:16px;
	cursor:pointer;
	right:12px;
	top:12px;
	background:url("/common/images/global/lg-global-sprite.png") 0 -174px no-repeat;
}

#hero-area .tabs div.active .xout{
	display:block;
}

#hero-area .tabs ul {
	list-style: none;
	width:250px;
	margin:0 auto;
}

#hero-area .tabs li {
	margin-bottom: 0;
	margin-top: 5px;
}

#hero-area .tabs h3 {
	padding-left:12px;
	text-transform:uppercase;
	color:#5d5c5c;
	font-size:12px;
	margin-bottom:10px;
	cursor:pointer;
}

#hero-area .tabs p {
	padding-left:15px;
}

#hero-area .tabs a:link, #hero-area .tabs a:visited {
	color:#D80546;
}

#hero-area .tabs .cta-link {
	float:right;
}

#hero-area .tabs close-link {
	clear:both;
	position:absolute;
	bottom:0px;
	right:20px;
	line-height:12px;
	background:url("/common/images/global/lg-global-sprite.png") top right no-repeat;
	padding-right:18px;
}

#hero-area .tabs .close-link a:link, #hero-area .tabs .close-link a:visited {
	color:#727272;
}

/* No longer need last -tab declerations
#hero-area .tabs div.last-tab {
	border-right:none;
}
